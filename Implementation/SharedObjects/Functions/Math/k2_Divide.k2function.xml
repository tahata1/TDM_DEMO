  <Desc>k2_Divide first number by second number.
 If the second number is zero, NULL is returned</Desc>
  <Language>PHP</Language>
  <FunctionBody>if (is_null($i_num_1) || is_null($i_num_2)){
	return null;
} 
elseif ($i_num_2 == 0) {
		return null;
} else
	return $i_num_1 / $i_num_2;</FunctionBody>
